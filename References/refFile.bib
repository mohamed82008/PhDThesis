@article{Moh1996,
abstract = {In this paper, the floorplanning problem for a layout problem is$\backslash$nformulated as a global optimization problem. The area of each building$\backslash$nblock is assumed to be fixed. However, its width and height are allowed$\backslash$nto vary subject to aspect ratio constraints. Also, a block may be$\backslash$narbitrarily oriented in parallel to the xy orthogonal axes, subject to$\backslash$npartition constraints with associated adjacency relationships. The$\backslash$nobjective is to minimize the rectangular area of the entire layout. By$\backslash$nformulating the problem appropriately, it becomes a geometric$\backslash$nprogramming problem. Its global minimum can then be found by using$\backslash$nstandard convex optimization techniques. The problem formulation and its$\backslash$nconversion to a convex optimization problem are first illustrated$\backslash$nthrough a simple example. The general procedure is then described and$\backslash$nthe effectiveness of the approach demonstrated through numerical$\backslash$nexamples},
