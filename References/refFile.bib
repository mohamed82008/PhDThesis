volume = {35},
year = {2008}
}
@article{Jakiela2000,
abstract = {The genetic algorithm (GA), an optimization technique based on the$\backslash$ntheory of natural selection, is applied to structural topology design$\backslash$nproblems. After reviewing the GA and previous research in structural$\backslash$ntopology optimization, we describe a binary material/void design$\backslash$nrepresentation that is encoded in GA chromosome data structures.$\backslash$nThis representation is intended to approximate a material continuum$\backslash$nas opposed to discrete truss structures. Four examples, showing the$\backslash$nbroad utility of the approach and representation, are then presented.$\backslash$nA fifth example suggests an alternate representation that allows$\backslash$ncontinuously-variable material density. Concluding discussion suggests$\backslash$nrecommended uses of the technique and describes ongoing and possible$\backslash$nfuture work. (C) 2000 Elsevier Science S.A. All rights reserved},
