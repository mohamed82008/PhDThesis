@article{Bartlett2000,
abstract = {We consider a comparison of active set vs. interior point$\backslash$nstrategies for the solution of receding time horizon problems in$\backslash$nnonlinear model predictive control (NMPC). For this study we consider a$\backslash$ncontrol algorithm where we form quadratic programs (QPs) in each time$\backslash$nhorizon by linearizing the model. We also ignore second order$\backslash$ninformation on the model and constraints. This approach can be viewed as$\backslash$na direct nonlinear extension of MPC with linear models and is easily$\backslash$ntailored to include stabilizing constraints. Using this framework we$\backslash$nconsider the application of three active set strategies as well as$\backslash$ninterior point methods applied to both the NMPC and the QP subproblem.$\backslash$nThe first two active set methods (QPOPT and and QKWIK) are general$\backslash$npurpose solvers that have been incorporated into SQP algorithms$\backslash$npreviously, while the third is a Schur complement approach that can$\backslash$neasily exploit the sparse structure of the KKT matrix in MPC},
author = {Bartlett, R.a. and Wachter, A and Biegler, L.T.},
file = {:E$\backslash$:/Literature/Optimization/Control/Active Set vs Interior Point Strategies for Model Predictive Control.pdf:pdf},
journal = {Proceedings of the 2000 American Control Conference.(ACC )},
number = {6},
pages = {4229--4233},
title = {{Active set vs. interior point strategies for model predictive control}},
volume = {6},
year = {2000}
}
@article{Kirches2011,
abstract = {Quadratic programs obtained for optimal control problems of dynamic or discrete-time processes usually involve highly block structured Hessian and constraints matrices, to be exploited by efficient numerical methods. In interior point methods, this is elegantly achieved by the widespread availability of advanced sparse symmetric indefinite factorization codes. For active set methods, however, conventional dense matrix techniques suffer from the need to update base matrices in every active set iteration, thereby loosing the sparsity structure after a few updates. This contribution presents a new factorization of a KKT matrix arising in active set methods for optimal control. It fully respects the block structure without any fill-in. For this factorization, matrix updates are derived for all cases of active set changes. This allows for the design of a highly efficient block structured active set method for optimal control and model predictive control problems with long horizons or many control parameters.},
author = {Kirches, Christian and Bock, Hans Georg and Schl{\"{o}}der, Johannes P. and Sager, Sebastian},
file = {:E$\backslash$:/Literature/Optimization/Control/A factorization with update procedures for a KKT matrix arising in direct optimal control.pdf:pdf},
journal = {Mathematical Programming Computation},
keywords = {Block structured active set quadratic programming,Direct methods for optimal control,Matrix factorizations and updates},
number = {4},
pages = {319--348},
title = {{A factorization with update procedures for a KKT matrix arising in direct optimal control}},
volume = {3},
year = {2011}
}
@article{Gill2005,
abstract = {Sequential quadratic programming (SQP) methods have proved highly effective for solving constrained optimization problems with smooth nonlinear functions in the objective and constraints. Here we consider problems with general inequality constraints (linear and nonlinear). We assume that first derivatives are available and that the constraint gradients are sparse.We discuss an SQP algorithm that uses a smooth augmented Lagrangian merit function and makes explicit provision for infeasibility in the original problem and the QP subproblems. SNOPT is a particular implementation that makes use of a semidefinite QP solver. It is based on a limited-memory quasi-Newton approximation to the Hessian of the Lagrangian and uses a reduced-Hessian algorithm (SQOPT) for solving the QP subproblems. It is designed for problems with many thousands of constraints and variables but a moderate number of degrees of freedom (say, up to 2000). An important application is to trajectory optimization in the aerospace industry. Nume...},
author = {Gill, Philip E. and Murray, Walter and Saunders, Michael A.},
journal = {SIAM Review},
keywords = {10,1137,49d37,49j15,49j20,49m37,65f05,65k05,90c30,ams subject classifications,doi,large-scale optimization,limited-memory methods,nonlinear inequality constraints,nonlinear programming,quasi-newton methods,quential quadratic programming,s0036144504446096,se-},
number = {1},
pages = {99--131},
title = {{SNOPT: An SQP Algorithm for Large-Scale Constrained Optimization}},
volume = {47},
year = {2005}
}
@article{Cheng2015,
abstract = {Although robust optimization can find solutions to engineering problems with uncertainty, the computa-tional inefficiency of robust optimization methods is still a major concern. In this article, a new hybrid robust optimization method, namely differential evolution–sequential quadratic programming–robust optimiza-tion (DE-SQP-RO), is presented to find global robust optima for nonlinear robust optimization problems. The proposed algorithm is conducted under the structure of DE-RO, with SQP-RO acting as a local opti-mizer. Two criteria and switch indices are developed to indicate when the algorithm should switch from DE-RO to SQP-RO and vice versa. One numerical and two engineering examples are tested to demonstrate the applicability of the proposed algorithm. The results show that the hybrid algorithm uses 29–45{\%} of the number of function evaluations required by DE-RO, and the robust solutions obtained by the hybrid algorithm are even better for certain examples.},
author = {Cheng, Shuo and Li, Mian},
journal = {Engineering Optimization},
keywords = {DE,SQP,hybrid algorithm,interval uncertainty,robust optimization},
number = {1},
pages = {87--106},
title = {{Robust optimization using hybrid differential evolution and sequential quadratic programming}},
volume = {47},
year = {2015}
}
