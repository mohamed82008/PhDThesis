keywords = {Buckling,Nonconvex semidefinite programming,Stability control,Structural optimization},
number = {5 SPEC. ISS.},
pages = {595--609},
title = {{Solving nonconvex SDP problems of structural optimization with stability control}},
volume = {19},
year = {2004}
}
