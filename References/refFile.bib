@article{Alvarez2005,
abstract = {We show that a problem of finding the truss of minimum expected compliance$\backslash$nunder stochastic loading conditions is equivalent to the dual of$\backslash$na special convex minimax problem, and therefore may be efficiently$\backslash$nsolved. This equivalence makes it possible to provide classic multiload$\backslash$ncompliance minimization problems with interpretations in a probabilistic$\backslash$nsetting. In fact, we prove that minimizing the expected compliance$\backslash$namounts to solving a multiload-like problem associated with a particular$\backslash$nfinite set of loading scenarios, which depend on the mean and the$\backslash$nvariance of the perturbations.},
