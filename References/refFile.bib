@article{Rojas-Labanda2015,
abstract = {Structural topology optimization problems are often modelled using$\backslash$nmaterial interpolation schemes to produce almost solid-and-void designs.$\backslash$nThe problems become non convex due to the use of these techniques.$\backslash$nSeveral articles introduce continuation approaches in the material$\backslash$npenalization parameter to reduce the risks of ending in local minima.$\backslash$nHowever, the numerical performance of continuation methods has not been$\backslash$nstudied in detail. The first purpose of this article is to benchmark$\backslash$nexisting continuation methods and the classical formulation with fixed$\backslash$npenalty parameter in structural topology optimization. This is done$\backslash$nusing performance profiles on 225 minimum compliance and 150 compliant$\backslash$nmechanism design problems. The results show that continuation methods$\backslash$ngenerally find better designs. On the other hand, they typically require$\backslash$na larger number of iterations. In the second part of the article this$\backslash$nissue is addressed. We propose an automatic continuation method, where$\backslash$nthe material penalization parameter is included as a new variable in the$\backslash$nproblem and a constraint guarantees that the requested penalty is$\backslash$neventually reached. The numerical results suggest that this approach is$\backslash$nan appealing alternative to continuation methods. Automatic continuation$\backslash$nalso generally obtains better designs than the classical formulation$\backslash$nusing a reduced number of iterations.},
author = {Rojas-Labanda, Susana and Stolpe, Mathias},
file = {:E$\backslash$:/Literature/Optimization/Applications/Shape Optimization and Material Selection/SIMP/Automatic penalty continuation in structural topology.pdf:pdf},
journal = {Structural and Multidisciplinary Optimization},
keywords = {Benchmarking,Continuation methods,Mechanism design,Minimum compliance,Topology optimization},
number = {6},
pages = {1205--1221},
title = {{Automatic penalty continuation in structural topology optimization}},
volume = {52},
year = {2015}
}
