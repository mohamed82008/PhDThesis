keywords = {Evolutionary topology optimization,GPU computing,Isosurfaces,Large scale,Multigrid preconditioning},
pages = {119--136},
title = {{GPU acceleration for evolutionary topology optimization of continuum structures using isosurfaces}},
volume = {182},
year = {2017}
}
