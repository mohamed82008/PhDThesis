file = {:E$\backslash$:/PhD/LiteratureReview/Robust/Load uncertainty/Mean compliance minimization/Stochastic sampling for deterministic structural topology optimization with many load cases Density-based and ground structure approaches.pdf:pdf},
issn = {00457825},
journal = {Computer Methods in Applied Mechanics and Engineering},
keywords = {Density-based method,Ground structure method,Randomized algorithm,Stochastic sampling,Topology optimization with many load cases,Trace estimator},
pages = {463--487},
publisher = {Elsevier B.V.},
title = {{Stochastic sampling for deterministic structural topology optimization with many load cases: Density-based and ground structure approaches}},
url = {http://dx.doi.org/10.1016/j.cma.2017.06.035},
volume = {325},
year = {2017}
}
