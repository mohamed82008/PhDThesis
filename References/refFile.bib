url = {http://joss.theoj.org/papers/10.21105/joss.00615},
volume = {3},
year = {2018}
}
@article{Emden2003,
abstract = {We investigate unconstrained optimization with an objective function that has an unknown and possibly large number of local minima. By varying the selection and termination criteria, we obtain several variants of the Moore-Skelboe algorithm for distinct tasks in nonconvex global optimization. All of these terminate after having found the best answer that is possible, given the precision of the underlying hardware and given the expression for the objective function. The first algorithm finds the best lower bound for the global minimum. This is then extended to a version that adds an upper bound. Often not only the global minimum is required, but also possibly existing points that achieve near-optimality, yet are far from the points at which the global minimum occurs. In response to this requirement we define the AE-minimizer, the set of points at which the objective function is within AE of the global minimum. We then present algorithms that return a set of boxes. In one of these, the union of the boxes in this set contains a AE-minimizer. If this union is small, then we know that there is a well-defined global minimum. In the other version, the union of the boxes returned is contained in a AE-minimizer. If this union is large, then we know that there is a wide choice of parameters that yield near-optimal objective function values.},
author = {Emden, M H Van and Moa, B.},
file = {:C$\backslash$:/Users/user/Downloads/mooreSkelb.pdf:pdf},
journal = {Frontiers in Global Optimization},
keywords = {mini-,mizer,moore-skelboe algorithm,nonconvex unconstrained global optimization,sensitivity analysis},
pages = {585--597},
title = {{Termination criteria in the Moore-Skelboe Algorithm for Global Optimization by Interval Arithmetic}},
year = {2003}
}
