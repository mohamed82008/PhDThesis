@article{Amir2014,
abstract = {This article presents a computational approach that facilitates the$\backslash$nefficient solution of 3-D structural topology optimization problems$\backslash$non a standard PC. Computing time associated with solving the nested$\backslash$nanalysis problem is reduced significantly in comparison to other$\backslash$nexisting approaches. The cost reduction is obtained by exploiting$\backslash$nspecific characteristics of a multigrid preconditioned conjugate$\backslash$ngradients (MGCG) solver. In particular, the number of MGCG iterations$\backslash$nis reduced by relating it to the geometric parameters of the problem.$\backslash$nAt the same time, accurate outcome of the optimization process is$\backslash$nensured by linking the required accuracy of the design sensitivities$\backslash$nto the progress of optimization. The applicability of the proposed$\backslash$nprocedure is demonstrated on several 2-D and 3-D examples involving$\backslash$nup to hundreds of thousands of degrees of freedom. Implemented in$\backslash$nMATLAB, the MGCG-based program solves 3-D topology optimization problems$\backslash$nin a matter of minutes. This paves the way for efficient implementations$\backslash$nin computational environments that do not enjoy the benefits of high$\backslash$nperformance computing, such as applications on mobile devices and$\backslash$nplug-ins for modeling software.},
author = {Amir, Oded and Aage, Niels and Lazarov, Boyan S.},
file = {:E$\backslash$:/Literature/Optimization/Applications/Shape Optimization and Material Selection/Multigrid preconditioner/On the multigrid-CG for efficient topology optimization.pdf:pdf},
journal = {Structural and Multidisciplinary Optimization},
keywords = {Multigrid,Preconditioned conjugate gradients,Topology optimization},
number = {5},
pages = {815--829},
title = {{On multigrid-CG for efficient topology optimization}},
volume = {49},
year = {2014}
}
@article{Deb2002,
abstract = {MuItiobjective evolutionary algorithms (EAs) that use nondominated sorting and sharing have been criticized mainly for their: 1) O(MN3) computational complexity (where M is the number of objectives and N is the population size); 2) nonelitism approach; and 3) the need for specifying a sharing parameter. In this paper, we suggest a nondominated sorting-based multiobjective EA (MOEA), called nondominated sorting genetic algorithm Il (NSGA-II). which alleviates all the above three difficulties. Specifically, a fast nondominated sorting approach with O(MN2) computational complexity is presented. Also, a selection operator is presented that creates a mating pool by combining the parent and offspring populations and selecting the best (with respect to fitness and spread) N solutions. Simulation results on difficult test problems show that the proposed NSGA-II, in most problems, is able to find much better spread of solutions and better convergence near the true Pareto-optimal front compared to Pareto-archived evolution strategy and strength-Pareto EA-two other elitist MOEAs that pay special attention to creating a diverse Pareto-optimal front. Moreover, we modify the definition of dominance in order to solve constrained multiobjective problems efficiently. Simulation results of the constrained NSGA-II on a number of test problems, including a five-objective seven-constraint nonlinear problem, are compared with another constrained muItiobjective optimizer and much better performance of NSGA-II is observed.},
author = {Deb, K and Pratab, S and Agarwal, S and Meyarivan, T},
file = {:E$\backslash$:/Literature/Optimization/Applications/Shape Optimization and Material Selection/Multiobjective/A fast and elitist multiobjective genetic algorithm NSGA-II.pdf:pdf},
journal = {IEEE Transactions on Evolutionary Computing},
number = {2},
pages = {182--197},
title = {{A Fast and Elitist Multiobjective Genetic Algorithm: NGSA-II}},
volume = {6},
year = {2002}
}
